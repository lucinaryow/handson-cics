         DFHMDF INITIAL='SAMM30',                                      C
               POS=(1,1),                                              C
               LENGTH=6,                                               C
               ATTRB=ASKIP
         DFHMDF INITIAL='REALCICS NAME && ADDRESS SYSTEM',             C
